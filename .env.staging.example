# Staging Environment
NODE_ENV=staging
PORT=5000

# Redis (Cluster)
REDIS_URL=rediss://redis-staging:6379
REDIS_CLUSTER_NODES=redis1:6379,redis2:6379,redis3:6379
REDIS_USERNAME=default
REDIS_PASSWORD=__set_in_secret__
REDIS_CA_PATH=/var/run/secrets/redis/ca.crt
REDIS_CERT_PATH=/var/run/secrets/redis/tls.crt
REDIS_KEY_PATH=/var/run/secrets/redis/tls.key
REDIS_SERVERNAME=redis-staging

# Observability
ENABLE_METRICS=1
METRICS_KEY=__set_in_secret__
METRICS_ALLOW_FROM=
HEALTH_KEY=__set_in_secret__

# Features
ENABLE_QUEUES=0
ENABLE_SESSIONS=0
ENABLE_STAT_GATING=0

# Monitoring
APP_VERSION=1.3.2
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# CORS
CLIENT_URL=https://staging.yourdomain.com
CORS_ORIGIN=https://staging.yourdomain.com