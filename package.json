    "drizzle-zod": "^0.7.1",
    "embla-carousel-react": "^8.6.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "framer-motion": "^11.13.1",
    "immer": "^10.1.1",
    "input-otp": "^1.4.2",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.453.0",
    "memorystore": "^1.6.7",
    "mysql2": "^3.14.3",
    "nats": "2.16.0",
    "next-themes": "^0.4.6",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "prom-client": "^15.1.3",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
