    "@radix-ui/react-tooltip": "^1.2.0",
    "@react-spring/web": "^10.0.1",
    "@slack/webhook": "^7.0.5",
    "@tanstack/react-query": "^5.60.5",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/uuid": "^10.0.0",
    "@use-gesture/react": "^10.3.1",
    "bullmq": "^5.56.9",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
